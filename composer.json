    "description": "VIVINIKO Media Service Provider",
	"keywords": ["viviniko", "media", "laravel"],
	"homepage": "http://viviniko.com",
	"license": "MIT",
	"authors": [
        {
            "name": "viviniko",
            "email": "viviniko.shop@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "aliyuncs/oss-sdk-php": "~2.0",
        "illuminate/http": "^6.0",
        "intervention/image": "^2.5",
        "viviniko/repository": "dev-master",
        "viviniko/support": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Viviniko\\Media": "src"
        }
